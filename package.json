{
  "name": "tabsets",
  "version": "0.4.5",
  "description": "Bookmarks next generation - Tabsets is a browser extension which helps you get your links really organized.",
  "productName": "Tabsets - Manage Tabs and Bookmarks with Ease",
  "author": "evandor <evandor@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m bex",
    "dev spa (only for testing)": "quasar dev -m spa",
    "dev pwa (only for testing)": "quasar dev -m pwa",
    "dev stage prod": "stage=production quasar dev -m bex",
    "dev electron": "quasar dev -m electron",
    "build": "quasar build -m bex",
    "build electron": "quasar build -m electron",
    "icon": "icongenie generate -i src/assets/icon.png",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:watch": "vitest --watch",
    "test:unit:watchAll": "vitest --watchAll",
    "test:e2e": "npx playwright test",
    "test:e2e:showReport": "npx playwright show-report",
    "test:specificTest": "npx playwright test addTab",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.27.2",
    "@editorjs/header": "^2.7.0",
    "@extractus/feed-extractor": "^7.0.1",
    "@mozilla/readability": "^0.4.4",
    "@quasar/extras": "^1.16.4",
    "@recogito/recogito-js": "^1.8.2",
    "@types/lodash": "^4.14.182",
    "@types/sanitize-html": "^2.9.0",
    "@xenova/transformers": "^2.2.0",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "fuse.js": "^6.6.2",
    "html-to-text": "^9.0.4",
    "idb": "^7.0.2",
    "lodash": "^4.17.21",
    "mhtml2html": "^3.0.0",
    "pinia": "^2.0.11",
    "quasar": "^2.12.0",
    "sanitize-html": "^2.10.0",
    "throttled-queue": "^2.1.4",
    "uuid": "^9.0.0",
    "vite-plugin-package-version": "^1.0.2",
    "vue": "3",
    "vue-draggable-next": "^2.1.1",
    "vue-i18n": "^9.0.0",
    "vue-json-pretty": "^2.2.3",
    "vue-konva": "^3.0.2",
    "vue-router": "^4.0.0",
    "vue-timeago3": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@playwright/test": "^1.27.1",
    "@quasar/app-vite": "^1.4.3",
    "@quasar/babel-preset-app": "^2.0.2",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.3.0",
    "@types/node": "^20.4.10",
    "@types/uuid": "^9.0.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.1",
    "@vue/test-utils": "^2.0.0",
    "@wdio/cli": "^8.13.5",
    "autoprefixer": "^10.4.2",
    "canvas": "^2.11.0",
    "electron": "^25.5.0",
    "electron-packager": "^17.1.1",
    "eslint-plugin-jest": "^27.1.3",
    "fake-indexeddb": "^4.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
