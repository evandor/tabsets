{
  "name": "tabsets",
  "version": "0.2.9",
  "description": "Browser Extension to manage your tabs by grouping them into re-storable sets",
  "productName": "Tabsets Extension",
  "author": "evandor <evandor@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m bex",
    "dev stage prod": "stage=production quasar dev -m bex",
    "build": "quasar build -m bex",
    "icon": "icongenie generate -i src/assets/icon.png",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ui": "majestic",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "test:e2e": "npx playwright test",
    "test:e2e:showReport": "npx playwright show-report",
    "test:specificTest": "npx playwright test addTabset",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@extractus/feed-extractor": "^6.1.7",
    "@quasar/extras": "^1.15.9",
    "@types/lodash": "^4.14.182",
    "apexcharts": "^3.36.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "fabric": "^5.2.4",
    "firebase": "^9.9.2",
    "firebaseui": "^6.0.1",
    "fuse.js": "^6.6.2",
    "html-to-text": "^8.2.1",
    "idb": "^7.0.2",
    "lodash": "^4.17.21",
    "logging-library": "^0.5.0",
    "mhtml2html": "^3.0.0",
    "pinia": "^2.0.11",
    "quasar": "^2.11.3",
    "throttled-queue": "^2.1.4",
    "vite-plugin-package-version": "^1.0.2",
    "vue": "3",
    "vue-draggable-next": "^2.1.1",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
    "vue-timeago3": "^2.3.0",
    "vue3-apexcharts": "^1.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@playwright/test": "^1.27.1",
    "@quasar/app-vite": "^1.1.3",
    "@quasar/babel-preset-app": "^2.0.1",
    "@quasar/quasar-app-extension-testing": "^2.0.6",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-beta.5",
    "@types/fabric": "^4.5.12",
    "@types/node": "^12.20.21",
    "@vue/test-utils": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "electron": "^20.2.0",
    "electron-packager": "^15.2.0",
    "eslint-plugin-jest": "^27.1.3",
    "fake-indexeddb": "^4.0.0",
    "jest": "^29.2.2",
    "jest-chrome": "^0.7.2",
    "majestic": "^1.7.0",
    "typescript": "^4.5.4",
    "vue3-jest": "^27.0.0-alpha.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
