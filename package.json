    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "diff": "^7.0.0",
    "dotenv": "^16.4.7",
    "events": "^3.3.0",
    "fuse.js": "^7.0.0",
    "html-to-text": "^9.0.5",
    "idb": "^8.0.0",
