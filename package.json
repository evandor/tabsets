    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "fake-indexeddb": "^6.0.0",
    "globals": "^15.13.0",
    "mermaid": "^11.4.1",
    "prettier": "^3.4.2",
    "typescript": "~5.5.4",
    "vite-plugin-checker": "^0.8.0",
    "vitepress": "^1.5.0",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "workbox-build": "^7.3.0",
