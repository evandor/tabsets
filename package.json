    "@quasar/babel-preset-app": "^2.0.3",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vue/devtools": "^7.4.6",
