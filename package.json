    "vue-i18n": "^10.0.3",
    "vue-json-pretty": "^2.4.0",
    "vue-router": "^4.4.3",
    "vue-timeago3": "^2.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@capacitor/cli": "^6.1.2",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/image": "^2.10.0",
    "@editorjs/link": "^2.6.2",
