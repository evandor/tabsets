{
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "permissions": [
    "tabGroups",
    "tabs",
    "activeTab",
    "alarms",
    "contextMenus",
    "scripting",
    "notifications",
    "webRequest",
    "pageCapture",
    "search",
    "sidePanel"
  ],
  "optional_permissions": [
    "bookmarks",
    "history"
  ],
  "host_permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "side_panel": {
    "default_path": "www/sidepanel.html"
  },
  "action": {
    "default_title": "Start Tabsets Extension",
    "default_icon": {
      "16": "icons/icon-16x16.png",
      "48": "icons/icon-48x48.png",
      "128": "icons/icon-128x128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
